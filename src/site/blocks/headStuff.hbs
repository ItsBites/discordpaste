<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/codemirror.min.css" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/codemirror.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/addon/edit/matchbrackets.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/addon/scroll/simplescrollbars.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/addon/scroll/simplescrollbars.min.css" />

<script>
	{{#if user}}window.user = true{{/if}}
	{{#if options.mode}}localStorage.mode = "{{options.mode}}"{{/if}}
	{{#if options.tabType}}localStorage.tabType = "{{options.tabType}}"{{/if}}
	{{#if options.tabSize}}localStorage.tabSize = "{{options.tabSize}}"{{/if}}
	{{#if options.theme}}localStorage.theme = "{{options.theme}}"{{/if}}
	{{#if options.keyMap}}localStorage.keymap = "{{options.keyMap}}"{{/if}}

	window.options = {
		lineNumbers: true,
		indentWithTabs: true,
		tabSize: 2,
		autofocus: true,
		theme: "pastel-on-dark",
		mode: "text",
		keyMap: "default",
		readOnly: {{#if view}}true{{else}}false{{/if}}
	}

	if(options.readOnly && localStorage.mode) {
		options.mode = localStorage.mode;
	}

	if(localStorage.tabType === "space") {
		options.indentWithTabs = false;
	}

	if(localStorage.tabSize) {
		options.tabSize = parseInt(localStorage.tabSize);
	}

	if(localStorage.theme) {
		options.theme = localStorage.theme;
		if(options.theme !== "default") {
			$("<link>").attr("rel", "stylesheet").attr("href", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/theme/" + options.theme + ".min.css").appendTo("head");
		}
	} else {
		$("<link>").attr("rel", "stylesheet").attr("href", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/theme/pastel-on-dark.min.css").appendTo("head");
	}

	if(localStorage.keyMap) {
		options.keyMap = localStorage.keyMap;
		if(options.keyMap !== "default") {
			$("<link>").attr("rel", "stylesheet").attr("href", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/keymap/" + options.theme + ".min.js").appendTo("head");
		}
	}

	if(options.mode === "htmlembbeded" || options.mode === "htmlmixed") {
		$("<script>").attr("src", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/mode/xml/xml.min.js").appendTo("head");
		$("<script>").attr("src", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/mode/javascript/javascript.min.js").appendTo("head");
		$("<script>").attr("src", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/mode/css/css.min.js").appendTo("head");
	} else if(options.mode === "skript") {
		$("<script>").attr("src", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/addon/mode/simple.min.js").appendTo("head");
		$("<script>").attr("src", "{{baseURL}}/assets/js/skriptMode.js").appendTo("head");
	} else if(options.mode !== "text") {
		$("<script>").attr("src", "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.28.0/mode/{{lang}}/{{lang}}.min.js").appendTo("head");
	}
</script>

